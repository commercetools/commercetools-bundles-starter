{
  "name": "@custom-applications-local/bundles-dynamic",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production mc-http-server --config=$(pwd)/env.json --use-local-assets",
    "compile-html:aws": "mc-scripts compile-html --headers=$(pwd)/production-aws.headers.json --config=$(pwd)/production-aws.env.json --use-local-assets --transformer $(pwd)/config/transformer-aws.js",
    "compile-html:firebase": "cross-env FIREBASE_TARGET=dynamic-bundle-manager mc-scripts compile-html --headers=$(pwd)/production-firebase.headers.json --config=$(pwd)/production-firebase.env.json --use-local-assets --transformer $(pwd)/config/transformer-firebase.js",
    "precommit": "lint-staged",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data '{src,../bundles-core,../core}/**/!(*.spec).js' --build-translations",
    "lint": "jest --projects jest.eslint.config.js jest.eslint-graphql.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "prettier --write '**/*.{js,css,gql,graphql,json}'",
    "format:js": "prettier --write '**/*.js'",
    "format:graphql": "prettier --write '**/*.{gql|graphql}'",
    "format:css": "prettier --write '**/*.css'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:coverage": "jest --config jest.test.config.js --coverage --collectCoverageFrom=src/components/**/*.js --collectCoverageFrom=!'src/**/(index|messages).js'",
    "deploy:firebase": "yarn compile-html:firebase && firebase deploy"
  },
  "dependencies": {
    "@apollo/react-components": "3.1.3",
    "@apollo/react-hooks": "3.1.3",
    "@commercetools-frontend/actions-global": "15.4.2",
    "@commercetools-frontend/application-components": "15.4.2",
    "@commercetools-frontend/application-shell": "15.4.2",
    "@commercetools-frontend/application-shell-connectors": "15.4.2",
    "@commercetools-frontend/assets": "15.4.2",
    "@commercetools-frontend/constants": "15.4.2",
    "@commercetools-frontend/i18n": "15.5.0",
    "@commercetools-frontend/l10n": "15.4.2",
    "@commercetools-frontend/mc-http-server": "15.5.0",
    "@commercetools-frontend/permissions": "15.4.2",
    "@commercetools-frontend/sdk": "15.4.2",
    "@commercetools-frontend/ui-kit": "10.13.0",
    "@custom-applications-local/bundles-core": "*",
    "@custom-applications-local/core": "*",
    "@types/react": "16.9.19",
    "apollo-cache": "1.3.4",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link": "1.2.13",
    "apollo-link-rest": "0.7.3",
    "apollo-utilities": "1.3.3",
    "formik": "2.1.2",
    "global": "4.4.0",
    "graphql": "14.6.0",
    "graphql-anywhere": "4.2.6",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "prop-types": "15.7.2",
    "qs": "6.9.1",
    "react": "16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.12.0",
    "react-intl": "3.6.0",
    "react-json-view": "1.19.1",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "yup": "0.28.0"
  },
  "devDependencies": {
    "@commercetools-frontend/eslint-config-mc-app": "15.4.2",
    "@commercetools-frontend/jest-preset-mc-app": "15.5.0",
    "@commercetools-frontend/mc-scripts": "15.5.0",
    "@testing-library/react": "9.4.0",
    "apollo-cache-inmemory": "1.6.5",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "enzyme": "3.11.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-graphql": "3.1.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "faker": "4.1.0",
    "graphql-tag": "2.10.1",
    "jest": "24.9.0",
    "jest-plugin-filename": "0.0.1",
    "jest-runner-eslint": "0.7.5",
    "jest-runner-stylelint": "2.2.3",
    "jest-validate": "24.9.0",
    "lint-staged": "10.0.2",
    "prettier": "1.19.1",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "4.0.0",
    "typescript": "3.7.5"
  }
}
