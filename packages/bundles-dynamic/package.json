{
  "name": "@commercetools-us-ps/mc-app-bundles-dynamic",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-http-server --use-local-assets",
    "compile-html:aws": "mc-scripts compile-html --headers=$(pwd)/production-aws.headers.json --config=$(pwd)/production-aws.env.json --use-local-assets --transformer $(pwd)/config/transformer-aws.js",
    "compile-html:firebase": "cross-env FIREBASE_TARGET=dynamic-bundle-manager mc-scripts compile-html --headers=$(pwd)/production-firebase.headers.json --config=$(pwd)/production-firebase.env.json --use-local-assets --transformer $(pwd)/config/transformer-firebase.js",
    "precommit": "lint-staged",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data '{src,../bundles-core,../core}/**/!(*.spec).js' --build-translations",
    "lint": "jest --projects jest.eslint.config.js jest.eslint-graphql.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "prettier --write '**/*.{js,css,gql,graphql,json}'",
    "format:js": "prettier --write '**/*.js'",
    "format:graphql": "prettier --write '**/*.{gql|graphql}'",
    "format:css": "prettier --write '**/*.css'",
    "test": "yarn run unit",
    "unit": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:coverage": "jest --config jest.test.config.js --coverage --collectCoverageFrom=src/components/**/*.js --collectCoverageFrom=!'src/**/(index|messages).js'",
    "deploy:firebase": "yarn compile-html:firebase && firebase deploy"
  },
  "dependencies": {
    "@apollo/client": "3.4.10",
    "@babel/runtime": "7.15.3",
    "@babel/runtime-corejs3": "7.15.3",
    "@commercetools-frontend/actions-global": "20.9.4",
