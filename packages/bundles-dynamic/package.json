{
  "name": "@commercetools-us-ps/mc-app-bundles-dynamic",
  "version": "1.2.0",
  "description": "Dynamic bundles custom application",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "compile-html": "mc-scripts compile-html",
    "compile-html:local": "MC_APP_ENV=development mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "npm run compile-html:local && mc-scripts serve",
    "test": "jest --config jest.test.config.js",
    "versions:appkit": "manypkg upgrade @commercetools-frontend",
    "versions:uikit": "manypkg upgrade @commercetools-uikit",
    "precommit": "lint-staged",
    "i18n:build": "formatjs extract --out-file=./src/i18n/data/core.json 'src/**/!(*.spec).js' '../bundles-core/**/!(*.spec).js'",
    "check": "yarn run lint && prettier --check '**/*.{js,css,gql,graphql,json}'",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "prettier --write '**/*.{js,css,gql,graphql,json}'",
    "test:unit": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:coverage": "jest --config jest.test.config.js --coverage --collectCoverageFrom=src/components/**/*.js --collectCoverageFrom=!'src/**/(index|messages).js'"
  },
