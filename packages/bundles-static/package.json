{
  "name": "@commercetools-us-ps/bundles-static",
  "version": "1.1.0",
  "description": "Static bundles custom application",
  "private": true,
  "scripts": {
    "build": "mc-scripts build --build-only",
    "start": "mc-scripts start",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "precommit": "lint-staged",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data '{src,../bundles-core,../core}/**/!(*.spec).js' --build-translations",
    "check": "yarn run lint && prettier --check '**/*.{js,css,gql,graphql,json}'",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:graphql": "jest --config jest.eslint-graphql.config.js --passWithNoTests",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "prettier --write '**/*.{js,css,gql,graphql,json}'",
    "test:unit": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "test:coverage": "jest --config jest.test.config.js --coverage --collectCoverageFrom=src/components/**/*.js --collectCoverageFrom=!'src/**/(index|messages).js'"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@commercetools-frontend/actions-global": "21.18.1",
    "@commercetools-frontend/application-components": "21.18.1",
    "@commercetools-frontend/application-shell": "21.18.1",
    "@commercetools-frontend/application-shell-connectors": "21.18.1",
    "@commercetools-frontend/assets": "21.18.1",
    "@commercetools-frontend/constants": "21.18.1",
    "@commercetools-frontend/i18n": "21.18.1",
    "@commercetools-frontend/l10n": "21.18.1",
    "@commercetools-frontend/permissions": "21.18.1",
    "@commercetools-frontend/sdk": "21.18.1",
    "@commercetools-frontend/ui-kit": "15.3.0",
    "@emotion/core": "^11.0.0",
    "@types/prop-types": "15.7.5",
    "@types/react": "16.14.34",
    "@types/react-dom": "17.0.18",
    "@types/react-redux": "7.1.24",
    "@types/react-router": "5.1.19",
    "@types/react-router-dom": "5.3.3",
    "apollo-cache": "1.3.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-link": "1.2.14",
    "apollo-link-rest": "0.8.0",
    "apollo-utilities": "1.3.4",
    "classnames": "2.3.2",
    "formik": "2.2.9",
    "global": "4.4.0",
    "graphql": "15.8.0",
    "graphql-anywhere": "4.2.8",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.38",
    "omit-empty": "1.0.0",
    "prop-types": "15.8.1",
    "qs": "6.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.25.1",
    "react-redux": "7.2.9",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "redux": "4.2.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commercetools-frontend/eslint-config-mc-app": "21.18.1",
    "@commercetools-frontend/jest-preset-mc-app": "21.18.1",
    "@commercetools-frontend/mc-scripts": "21.18.1",
    "@commercetools/enzyme-extensions": "5.0.0",
    "@commercetools/jest-enzyme-matchers": "1.1.2",
    "@testing-library/react": "12.1.5",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "apollo-cache-inmemory": "1.6.6",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-matchers": "7.1.2",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.27.0",
    "eslint-formatter-pretty": "4.1.0",
    "faker": "5.5.3",
    "graphql-tag": "2.12.6",
    "jest": "27.5.1",
    "jest-enzyme": "7.1.2",
    "jest-plugin-filename": "0.0.1",
    "jest-runner-eslint": "0.11.1",
    "jest-runner-stylelint": "2.3.7",
    "jest-validate": "27.5.1",
    "jscodeshift": "0.14.0",
    "lint-staged": "11.2.6",
    "prettier": "2.7.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "typescript": "4.8.4"
  }
}
